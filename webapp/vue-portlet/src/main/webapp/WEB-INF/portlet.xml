<portlet-app
  version="2.0"
  xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

  <portlet>
    <portlet-name>ProfileHamburgerMenu</portlet-name>
    <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/html/hamburgerMenuItem.jsp</value>
    </init-param>
    <expiration-cache>-1</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <portlet-info>
      <title>Profile Hamburger Menu</title>
    </portlet-info>
    <portlet-preferences>
      <preference>
        <name>jsModule</name>
        <value>PORTLET/social-vue-portlet/ProfileHamburgerMenu</value>
      </preference>
    </portlet-preferences>
  </portlet>

  <portlet>
    <portlet-name>SpacesHamburgerMenu</portlet-name>
    <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/html/hamburgerMenuItem.jsp</value>
    </init-param>
    <expiration-cache>-1</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <portlet-info>
      <title>Spaces Hamburger Menu</title>
    </portlet-info>
    <portlet-preferences>
      <preference>
        <name>jsModule</name>
        <value>PORTLET/social-vue-portlet/SpacesHamburgerMenu</value>
      </preference>
    </portlet-preferences>
  </portlet>

  <portlet>
    <portlet-name>SpacesAdministration</portlet-name>
    <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/spaces-administration-app/index.html</value>
    </init-param>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <supported-locale>en</supported-locale>
    <resource-bundle>locale.portlet.social.SpacesAdministrationPortlet</resource-bundle>
    <portlet-info>
      <title>Spaces Administration</title>
    </portlet-info>
  </portlet>

  <portlet>
    <portlet-name>SpaceTemplates</portlet-name>
    <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/space-templates-app/index.html</value>
    </init-param>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <supported-locale>en</supported-locale>
    <resource-bundle>locale.portlet.social.SpaceTemplatesPortlet</resource-bundle>
    <portlet-info>
      <title>Spaces Templates</title>
    </portlet-info>
  </portlet>

  <portlet>
    <portlet-name>TopBarLogo</portlet-name>
    <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/html/topbarLogo.html</value>
    </init-param>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <supported-locale>en</supported-locale>
    <resource-bundle>locale.portlet.social.SpaceTemplatesPortlet</resource-bundle>
    <portlet-info>
      <title>Top Bar Logo</title>
    </portlet-info>
  </portlet>

  <portlet>
    <portlet-name>TopBarNotification</portlet-name>
    <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
    <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/html/topbarNotification.html</value>
    </init-param>
    <supports>
      <mime-type>text/html</mime-type>
    </supports>
    <supported-locale>en</supported-locale>
    <resource-bundle>locale.portlet.social.SpaceTemplatesPortlet</resource-bundle>
    <portlet-info>
      <title>Top Bar Notification</title>
    </portlet-info>
  </portlet>
</portlet-app>
