<%
	String iconStatus = (uicomponent.isOnline()) ? "uiIconUserOnline" : "uiIconUserOffline";
%>
<div class="uiSocApplication uiStatusProfilePortlet" id="$uicomponent.id">
	 <h3 class="user-status" style="display:inline-block;" data-userid="<%=uicomponent.getCurrentRemoteId()%>">
	 	<span><%=uicomponent.currentProfile.getFullName()%></span>
	 <i class="$iconStatus uiIconLightGray"></i>
	 </h3>
</div>
